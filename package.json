{
  "name": "download-cdn",
  "version": "1.0.0",
  "description": "Manage cdn libs in one place, download them for local (test) use, and write them to html",
  "main": "download-cdn.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --timeout 10000 --reporter spec",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "ci-test": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmplank/download-cdn.git"
  },
  "keywords": [
    "cdn",
    "download",
    "local"
  ],
  "author": "Colin Plank",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmplank/download-cdn/issues"
  },
  "homepage": "https://github.com/cmplank/download-cdn#readme",
  "files": [
    "download-cdn.js"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "fs-copy-file": "^1.0.2",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2"
  }
}
